{
  "name": "react-recoil-form",
  "version": "0.2.4",
  "license": "MIT",
  "author": "Wit By Bit",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**",
    "src/**"
  ],
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/witbybit/react-recoil-form"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "build-ts": "tsc",
    "start": "yarn && start-storybook -p 6006",
    "prepare": "husky install",
    "format": "prettier . --write",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16",
    "recoil": ">=0.5.0"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/index.mjs",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.2",
    "@types/core-js": "^2.5.5",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "babel-jest": "^27.2.4",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "recoil": "0.6.1",
    "size-limit": "^5.0.5",
    "tslib": "^2.4.0",
    "tsup": "^6.2.1",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "core-js": "^3.18.1"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write"
  }
}
