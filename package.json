  "license": "MIT",
  "author": "Wit By Bit",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**",
    "src/**"
  ],
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/witbybit/react-recoil-form"
  },
  "scripts": {
    "build": "SET NODE_OPTIONS=--openssl-legacy-provider && tsup",
    "dev": "tsup --watch",
    "build-ts": "SET NODE_OPTIONS=--openssl-legacy-provider && tsc",
    "prepare": "husky install",
    "format": "prettier . --write",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "SET NODE_OPTIONS=--openssl-legacy-provider && start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16",
    "recoil": ">=0.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@size-limit/preset-small-lib": "^4.10.2",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-vite": "^0.2.2",
    "@storybook/builder-webpack4": "^6.5.10",
    "@storybook/manager-webpack4": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@types/core-js": "^2.5.5",
    "@types/react": "^17.0.11",
    "@types/react-beautiful-dnd": "^13.1.3",
    "@types/react-dom": "^17.0.8",
    "babel-jest": "^27.2.4",
    "babel-loader": "^8.2.5",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react": "^17.0.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^17.0.1",
    "recoil": "0.6.1",
    "size-limit": "^5.0.5",
    "tslib": "^2.4.0",
    "tsup": "^6.2.3",
    "typescript": "4.7.4",
    "vite": "^3.1.7"
  },
  "dependencies": {},
  "lint-staged": {
    "*": "prettier --ignore-unknown --write"
  }
